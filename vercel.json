{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(?:style\\.css|dashboard\\.js|netigo-logo\\.png)",
            "dest": "/public/$0"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}